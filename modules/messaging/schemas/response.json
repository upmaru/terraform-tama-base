    },
    "origin_entity_id": {
      "description": "The origin entity with the original message",
      "type": "string"
    },
    "messages": {
      "description": "Array of messages",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "role": {
            "type": "string"
          },
          "content": {
            "type": "string"
          }
        },
        "required": ["role", "content"]
      }
    }
  },
  "required": ["origin_entity_id", "forwarded_from_concept_id", "messages"],
  "title": "response",
  "type": "object"
}
